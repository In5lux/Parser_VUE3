{
  "name": "parser-vue-3",
  "version": "2.4.0",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server/index.js -- -server",
    "server": "cross-env NODE_ENV=development node server/index.js -- -server",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vue/typescript": "^1.8.8",
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "cron": "^2.4.3",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "nodemailer": "^6.9.4",
    "pinia": "^2.1.6",
    "pug": "^3.0.2",
    "puppeteer": "^21.1.1",
    "sirv": "^2.0.3",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "telegraf": "^4.13.1",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.0",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^20.5.9",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.48.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "~5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.3",
    "vue-tsc": "^1.8.8"
  }
}
